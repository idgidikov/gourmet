{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "prettier"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"semi": ["error", "always"],
		"quotes": ["error", "double"],
		"arrow-body-style": ["error", "as-needed"],
		"react/self-closing-comp": ["error", { "component": true, "html": true }]
	}
}
